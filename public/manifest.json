{
	"manifest_version": 3,
	
	"name": "웃긴대학 메이드",
	"author": "메우메우",
	"homepage_url": "http://web.humoruniv.com/",
	"description": "웃긴대학을 편리하게 이용할 수 있게 도와줍니다!",
	"version": "3.0.1",
	
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},

	"options_page": "options.html",

	"permissions": ["storage", "unlimitedStorage"],

	"content_scripts": [
		{
			"run_at": "document_start",
			"all_frames": false,
			"matches": ["<all_urls>"],
			"js": ["dist/pages.js", "dist/routes/test.js"]
		},
		{
			"run_at": "document_start",
			"all_frames": false,
			"matches": ["*://*.humoruniv.com/board/humor/write.html?table=picture"],
			"js": ["dist/pages.js", "dist/routes/humoruniv_write.js"]
		}
	]
}
